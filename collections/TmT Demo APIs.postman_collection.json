{
  "info": {
    "name": "TmT Demo APIs",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "API Landing Page",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"API Landing Page should render app name\", function () {",
              "    pm.expect(pm.response.code).to.eql(200);",
              "    pm.expect(pm.response.text()).to.include(\"Welcome\");",
              "});"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{protocol}}{{baseUrl}}",
          "host": ["{{protocol}}{{baseUrl}}"]
        }
      },
      "response": []
    }
  ],
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [""]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [""]
      }
    }
  ],
  "variable": [
    {
      "key": "demoKey",
      "value": "demoValue",
      "enabled": true
    },
    {
      "key": "baseUrl",
      "value": "localhost:3000",
      "enabled": true
    },
    {
      "key": "protocol",
      "value": "http://",
      "enabled": true
    }
  ],
  "protocolProfileBehavior": {}
}
